{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --dotenv .env.development.local --max-old-space-size=1024",
    "lint": "npx eslint ./ --ext .js,.vue --ignore-path .gitignore",
    "generate": "nuxt generate --dotenv .env.production.local",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "deploy": "cp .github/_config.yml dist/_config.yml && gh-pages -d dist"
  },
  "devDependencies": {
    "@nuxt/image-edge": "^1.0.0-27991759.1a5eea1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@pinia/testing": "^0.0.16",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-c8": "^0.29.8",
    "@vue/test-utils": "^2.3.2",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.10.0",
    "gh-pages": "^5.0.0",
    "happy-dom": "^9.1.9",
    "nuxt": "^3.4.2",
    "nuxt-vitest": "^0.6.9",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "stylelint": "^15.3.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1",
    "vitest": "^0.29.8"
  },
  "dependencies": {
    "@floating-ui/vue": "^0.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@nuxtjs/tailwindcss": "^6.6.4",
    "@pinia/nuxt": "^0.4.7",
    "@svgdotjs/svg.js": "^3.1.2",
    "pinia": "^2.0.33",
    "svg.js": "^2.7.1",
    "svgdom": "^0.1.13"
  },
  "overrides": {
    "vue": "latest"
  }
}
